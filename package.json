{
  "name": "countrtop-monorepo",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.10.0",
  "scripts": {
    "dev:customer-web": "pnpm --filter @countrtop/customer-web dev",
    "dev:customer": "pnpm --filter @countrtop/customer-mobile start",
    "dev:vendor-ops": "pnpm --filter @countrtop/vendor-ops-mobile start",
    "dev:vendor-admin": "pnpm --filter @countrtop/vendor-admin-web dev",
    "dev:kds": "pnpm --filter @countrtop/kds-web dev",
    "build:customer-web": "pnpm --filter @countrtop/customer-web build",
    "build:customer": "pnpm --filter @countrtop/customer-mobile build",
    "build:vendor-ops": "pnpm --filter @countrtop/vendor-ops-mobile build",
    "build:vendor-admin": "pnpm --filter @countrtop/vendor-admin-web build",
    "build:kds": "pnpm --filter @countrtop/kds-web build",
    "build": "pnpm -r run build",
    "lint": "pnpm -r run lint",
    "test": "pnpm -r test --if-present",
    "format": "prettier --write .",
    "poll:square": "tsx scripts/pollSquareOrders.ts"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "null-loader": "^4.0.1",
    "prettier": "^3.3.3",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3"
  }
}
